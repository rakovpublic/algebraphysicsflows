<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" name="groupimp" packages="">
    <Appenders>
        <File name="FileAppender" fileName="groupimp.log">
            <PatternLayout>
                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="FileAppender"/>
        </Root>
    </Loggers>
</Configuration>